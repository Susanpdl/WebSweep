{
  "manifest_version": 3,
  "name": "WebSweep - Browser History Analyzer",
  "version": "1.0",
  "description": "WebSweep helps you analyze and clean your browser history for better privacy and reduced digital footprint",
  "permissions": [
    "history",
    "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "WebSweep - Clean Your Digital Footprint"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 